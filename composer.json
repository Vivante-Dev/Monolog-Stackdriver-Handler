{
  "name": "vivante-health/Monolog-Stackdriver-Handler",
  "description": "Stackdriver Handler based on Monolog StreamHandler",
  "type": "library",
  "keywords": [
    "Stackdriver",
    "PHP",
    "log",
    "logging"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Eva Koliopoulou",
      "email": "ekoliopoulou@vivantehealth.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "monolog/monolog": "^1.23"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "VivanteHealth\\MonologStackdriverHandler\\": "src"
    }
  },
  "scripts": {
    "lint": "php ./vendor/bin/phpcs -p -s --standard=PSR12 ./src"
  }
}